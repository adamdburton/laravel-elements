{
  "bootstrap": "vendor/autoload.php",
  "path": "benchmarks",
  "extensions": [
    "PhpBench\\Extensions\\XDebug\\XDebugExtension"
  ],
  "reports": {
    "elements": {
      "extends": "aggregate",
      "break": ["groups", "benchmark"]
    }
  },
  "php_disable_ini": true,
  "php_config": {
    "sys_temp_dir": "/var/tmp"
  }
}
